{
  "name": "april-paradise",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "bun -b vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "pnpm biome lint . --apply",
    "format": "pnpm biome format . --write",
    "generate:edgeql": "pnpm exec generate queries",
    "generate:api": "bun run scripts/api-gen/index.ts",
    "gen": "pnpm run --parallel /^generate:/"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.4",
    "@edgedb/generate": "^0.4.1",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/enhanced-img": "^0.2.0",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/vite-plugin-svelte": "3.0.2",
    "@types/node": "^20.12.7",
    "lefthook": "^1.6.10",
    "sass": "^1.74.1",
    "svelte": "5.0.0-next.100",
    "svelte-check": "^3.6.9",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vitest": "^1.4.0"
  },
  "type": "module",
  "dependencies": {
    "@edgedb/auth-sveltekit": "^0.1.1",
    "@googleapis/sheets": "^5.0.5",
    "@radix-ui/colors": "^3.0.0",
    "@sentry/browser": "^7.110.0",
    "bits-ui": "^0.21.3",
    "devalue": "^4.3.2",
    "edgedb": "^1.5.0",
    "formsnap": "^1.0.0",
    "sveltekit-superforms": "^2.12.4",
    "zod": "^3.22.4"
  },
  "pnpm": {
    "patchedDependencies": {
      "@sveltejs/enhanced-img@0.2.0": "patches/@sveltejs__enhanced-img@0.2.0.patch",
      "@edgedb/generate@0.4.1": "patches/@edgedb__generate@0.4.1.patch"
    }
  }
}
